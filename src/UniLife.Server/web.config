<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<validation validateIntegratedModeConfiguration="false"/>
		<security>
			<requestFiltering>
				<requestLimits maxAllowedContentLength="1073741824" />
			</requestFiltering>
		</security>
	</system.webServer>
	<system.web>
		<httpRuntime maxRequestLength="2097151" executionTimeout="2097151" maxQueryStringLength="1048576" maxUrlLength="1048576" />
		<!--<httpRuntime maxQueryStringLength="1048576" maxUrlLength="1048576" />-->
	</system.web>
</configuration>